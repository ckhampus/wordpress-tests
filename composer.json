{
    "name": "ckhampus/wordpress-tests",
    "description": "Testing library for Wordpress",
    "authors": [
        {
            "name": "Cristian Hampus",
            "email": "contact@cristianhampus.se"
        }
    ],
    "require": {
        "symfony/http-foundation": "v2.1.1",
        "symfony/process": "v2.1.1",
        "symfony/filesystem": "v2.1.1",
        "symfony/finder": "v2.1.1",
        "symfony/yaml": "v2.1.1",
        "behat/mink": "v1.4.0",
        "behat/mink-zombie-driver": "*",
        "behat/mink-goutte-driver": "*",
        "behat/mink-selenium2-driver": "*",
        "behat/mink-sahi-driver": "*"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "dev-master"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Queensbridge": "src/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    }
